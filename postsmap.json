---
layout: none
---
[
  {% for post in site.posts %}
    {
      "author"    : "{{ post.author }}",
      "category"  : "{{ post.categories }}",
      "date"      : "{{ post.date }}",
      "impression": "{{ post.impression }}",
      "tags"      : "{{ post.tags | join: ', ' }}",
      "title"     : "{{ post.title | escape }}",
      "url"       : "{{ site.baseurl }}{{ post.url }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]